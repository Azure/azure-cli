[sa]
command = storage account

[storage account bsp]
command = storage account blob-service-properties

[storage account fsp]
command = storage account file-service-properties

[storage account service-properties show-blob]
command = storage account blob-service-properties show

[storage account service-properties show-file]
command = storage account file-service-properties show

[tsi]
command = timeseriesinsights

[timeseriesinsights env]
command = timeseriesinsights environment

[timeseriesinsights environment create]
command = timeseriesinsights environment longterm create
    [[--type {{type}}]]
    command = timeseriesinsights environment {{type}} create

[timeseriesinsights environment update]
command = timeseriesinsights environment longterm update
    [[--type {{type}}]]
    command = timeseriesinsights environment {{type}} update

