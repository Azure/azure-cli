 {
    "enabled": true,
    "type": "Inventory",
    "destination": "mycontainer",
    "rules": [
      {
        "enabled": true,
        "name": "inventoryPolicyRule1",
        "definition": {
          "filters": {
            "blobTypes": [
              "blockBlob"
            ],
            "prefixMatch": [
              "inventoryprefix1",
              "inventoryprefix2"
            ],
            "includeSnapshots": true,
            "includeBlobVersions": true
          }
        }
      }
    ]
  }

