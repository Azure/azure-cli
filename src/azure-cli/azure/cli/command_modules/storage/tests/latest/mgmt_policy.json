{
    "rules": [
        {
            "name": "olcmtest",
            "type": "Lifecycle",
            "definition": {
                "filters": {
                  "blobTypes": [
                    "blockBlob",
                    "appendBlob"
                  ],
                  "prefixMatch": [
                    "olcmtestcontainer1"
                  ]
                },
                "actions": {
                  "baseBlob": {
                    "delete": {
                      "daysAfterModificationGreaterThan": 90
                    }
                  },
                  "snapshot": {
                    "delete": {
                      "daysAfterCreationGreaterThan": 90
                    }
                  },
                  "version": {
                      "delete": {
                          "daysAfterCreationGreaterThan": 90
                      }
                  }
                }
            }
        }
    ]
}