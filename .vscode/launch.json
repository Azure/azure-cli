{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python",
			"type": "python",
			"request": "launch",
			"stopOnEntry": true,
            "pythonPath":"python",
			"program": "${workspaceRoot}/src/azure/cli/__main__.py",
            "cwd": "${workspaceRoot}/src",
            "args": ["login", "--help"],
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		}
	]
}